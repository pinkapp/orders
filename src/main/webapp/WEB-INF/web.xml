		<servlet-class>com.action.gonggao_servlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>gonggao</servlet-name>
		<url-pattern>/gonggao</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>news</servlet-name>
		<servlet-class>com.action.news_servlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>news</servlet-name>
		<url-pattern>/news</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>liuyan</servlet-name>
		<servlet-class>com.action.liuyan_servlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>liuyan</servlet-name>
		<url-pattern>/liuyan</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>zhuanye</servlet-name>
		<servlet-class>com.action.zhuanye_servlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>zhuanye</servlet-name>
		<url-pattern>/zhuanye</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>banji</servlet-name>
		<servlet-class>com.action.banji_servlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>banji</servlet-name>
		<url-pattern>/banji</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>tea</servlet-name>
		<servlet-class>com.action.tea_servlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>tea</servlet-name>
		<url-pattern>/tea</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>kecheng</servlet-name>
		<servlet-class>com.action.kecheng_servlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>kecheng</servlet-name>
		<url-pattern>/kecheng</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>stu</servlet-name>
		<servlet-class>com.action.stu_servlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>stu</servlet-name>
		<url-pattern>/stu</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>index</servlet-name>
		<servlet-class>com.action.index_servlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>index</servlet-name>
		<url-pattern>/index</url-pattern>
	</servlet-mapping>

	<servlet>
